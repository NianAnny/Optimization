    "import numpy as np\n",
    "\n",
    "A = np.array([[4,6,1,0,0,0],\n",
    "              [2,6,0,1,0,0],\n",
    "              [0,1,0,0,1,0],\n",
    "              [4,9,0,0,0,1]\n",
    "              ])\n",
    "\n",
    "b = np.array([120,72,10,84])\n",
    "\n",
    "basicVars = np.array([1,2,3,5]) \n",
    "B = A[:,basicVars]\n",
    "\n",
    "sol = np.linalg.solve(B,b)\n",
    "print(sol)"
